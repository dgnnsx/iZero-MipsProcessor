module multiplexador_bios(mem, bios, ctrl_mux_br, instrucao);
	// Entradas
	input [31:0] mem;					// Memoria de instrucoes
	input [31:0] bios;				// Bios
	
	// Controle
	input ctrl_mux_bios;				// Sinal de controle
	
	// Saida
	output [31:0] instrucao;		// Instrucao
	assign instrucao = ctrl_mux_bios ? mem : bios;
endmodule
